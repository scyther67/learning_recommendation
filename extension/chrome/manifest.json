{
    "name": "Recommender System",
    "version": "1.0",
    "description": "Personalized recommender system to learn SQL",
    "browser_action": {
      "default_title": "Recommender System",
      "default_popup":"popup.html"
    },
    "background": {
      "scripts": [
          "js/background.js"
        ],
      "persistent": false
    },
    "content_scripts":[{
        "js": ["js/content.js"],
        "matches": ["*://stackoverflow.com/*","*://*.stackoverflow.com/*","https://www.w3schools.com/sql/*"]
      },
      {
        "js": ["js/platform.js"],
        "matches": ["http://localhost/*"]
      }],
    "manifest_version": 2,
    "permissions":[
        "history",
        "tabs",
        "*://stackoverflow.com/*",
        "storage"
    ]
  }